%ul.captioned{ title: title }
  %li= download_link 'Low Res (20kb)', "logos/#{file}_#{title}_lr.png"
  %li= download_link 'Medium Res (600kb)', "logos/#{file}_#{title}_mr.png"
  %li= download_link 'High Res (2mb)', "logos/#{file}_#{title}_hr.png"
  %li= link_to 'Illustrator (1mb)', "##{file}.ai"
  %li= link_to 'EPS (1mb)', "##{file}.eps"
  %li= link_to 'PDF (1mb)', "##{file}.pdf"
  %li= link_to 'SVG (1mb)', "##{file}.svg"

